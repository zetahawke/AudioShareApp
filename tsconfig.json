{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "outDir": "./",
    "module": "es2015",
    "moduleResolution": "bundler",
    "target": "es2017",
    "allowJs": true,
    "esModuleInterop": true,
    "noEmit": false
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
