{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "es2015",
    "moduleResolution": "bundler",
    "target": "es2017",
    "allowJs": true,
    "esModuleInterop": true,
    "noEmit": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
